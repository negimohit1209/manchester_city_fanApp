{
  "extends": ["react-app", "eslint:recommended"],
  "rules": {
    "array-bracket-spacing": ["error", "never"],
    "comma-spacing": "error",
    "default-case": ["error"],
    "eol-last": "error",
    "eqeqeq": "error",
    "func-call-spacing": "error",
    "indent": [
      "warn",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "jsx-quotes": "error",
    "key-spacing": "error",
    "keyword-spacing": 1,
    "no-console": 0,
    "no-multi-spaces": "error",
    "no-sequences": "error",
    "no-use-before-define": "error",
    "no-unused-vars": "warn",
    "radix": "error",
    "semi": "error"
  }
}
